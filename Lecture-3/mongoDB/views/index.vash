<!DOCTYPE html>
<html>

<head>
  <meta>
  <title>@model.title</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1>@model.title</h1>
  <p>Welcome to @model.title</p>

  <ol>
    @model.courses.forEach(function(course){
      <li>@course.title</li>
    })
  </ol>
</body>

</html>