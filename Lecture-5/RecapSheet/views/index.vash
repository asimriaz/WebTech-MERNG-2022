<!DOCTYPE html>
<html>
<head>
  <meta>
  <title>@model.title</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <h1>@model.title</h1>
  <p>Welcome to @model.title</p>


  <table>
    <tr>
      <th>Name</th>
      <th>Regno</th>
      @model.heads.forEach(function(head){
        <th>@head.headname</th>
      })
    </tr>
    <tr>
      <th></th>
      <th></th>
      @model.heads.forEach(function(head){
        <th>@head.total</th>
      })
    </tr>
    @model.students.forEach(function(student){
      <tr>
        <td>@student.name</td>
        <td>@student.regno</td>
        @{
          student.scores.sort((a, b) => a.hid - b.hid);
        }
        @student.scores.forEach(function(mark){
          <td>@mark.marks</td>
        })
      </tr>
    })
  </table>

</body>
</html>
